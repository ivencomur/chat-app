{
  "expo": {
    "name": "Chat App",
    "slug": "chat-app",
    "version": "5.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#6200EE"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chatapp.exercise"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6200EE"
      },
      "package": "com.chatapp.exercise"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
